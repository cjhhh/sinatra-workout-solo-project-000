<h1> Edit a Single Workout </h1>

<p> <%= @workout.title %> </p>
<p> <%= @workout.content %> </p>


<form method="POST" action="/workouts/<%= @workout.id %>">
  <input type="hidden" name="_method" value="patch">
  <input type="text" name="title" id="content" value="<%= @workout.title %>"><br></br>
  <input type="text" name="content" id="content" value="<%= @workout.content %>"><br>
  <input type="submit" value="submit">
</form>
